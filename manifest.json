{
    "manifest_version": 2,
    "name": "TimeStamps",
    "version": "0.1",
    "author": "Nitin Verma",
    "description": "",
    "permissions": [
        "tabs"
    ],
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": [
            "content.js"
        ],
        "css": ["style.css"]
    }],
    "browser_action": {
        "default_popup": "index.html",
        "default_title": "A tooltip popup description when you hover over the Chrome Extension icon."
    }
}